vocabulary <http://opencaesar.io/template/vocabulary/vocabulary1> with # as vocabulary1 {
	
	// The vocabulary bundle used by this description bundle
	extends <http://www.w3.org/2002/07/owl>
	
	// Example aspect
	aspect A

	// Example concept
	concept C

	// Example relation entity
	relation entity R [
		from C
		to A
		forward r
	]

}
