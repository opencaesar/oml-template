PREFIX base:        <http://example.com/model/vocabulary/base#>
PREFIX mission:     <http://example.com/model/vocabulary/mission#>

SELECT DISTINCT ?m_id ?m_name
WHERE {
	?m a mission:Mission ;
		base:hasIdentifier ?m_id ;
		base:hasCanonicalName ?m_name ;
}
ORDER BY ?m_id

